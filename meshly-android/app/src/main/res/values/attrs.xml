<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Fonts">
        <attr name="font" format="string|reference" />
        <attr name="android:textStyle" />
        />
    </declare-styleable>



    <declare-styleable name="CustomTextView">
        <attr name="custom_font" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="custom_edit_text_font" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="MovingTextView">
        <attr name="moving_font" format="string|reference" />
    </declare-styleable>




    <declare-styleable name="LabelLayout">
        <attr name="item_space" format="dimension|reference" />
    </declare-styleable>

    <item type="id" name="fonts_extra_data" />








    <declare-styleable name="CustomSwitch">
        <attr name="thumb_color" format="color"/>
        <attr name="back_color" format="color"/>
        <attr name="back_color_pressed" format="color"/>
        <attr name="inner_padding" format="dimension"/>
        <attr name="round_radius" format="dimension"/>
    </declare-styleable>
<integer name="card_flip_time_full">300</integer>
<integer name="card_flip_time_half">150</integer>
</resources>